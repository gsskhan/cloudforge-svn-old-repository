<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<beans xmlns="http://www.springframework.org/schema/beans"		
	xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
							http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.2.xsd
							http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd">

	<!-- Register Annotation-based Post Processing Beans -->
	<context:annotation-config />	
	<bean id="cacheProperties"
        class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
        <property name="locations">
            <list>               
                <value>classpath:retail.optima.etl.sql.queries.properties</value>
            </list>
        </property>
    </bean>
    
    
	<bean id="fileIntegrityServiceRnbwFile" factory-bean="fileIntegrityServiceFactory" factory-method="createFileIntegrityService"	
	c:_0="RNBW" />
	
	<bean id="fileIntegrityServiceCntrlFile" factory-bean="fileIntegrityServiceFactory" factory-method="createFileIntegrityService"	
	c:_0="Control" />
	
	<bean id="fileIntegrityServiceEppFile" factory-bean="fileIntegrityServiceFactory" factory-method="createFileIntegrityService"	
	c:_0="EPP" />
	
	<bean id="fileIntegrityServiceFicoFile" factory-bean="fileIntegrityServiceFactory" factory-method="createFileIntegrityService"	
	c:_0="FICO" />
	
	<bean id="fileIntegrityServiceNtbFile" factory-bean="fileIntegrityServiceFactory" factory-method="createFileIntegrityService"	
	c:_0="NTB" />
	
	<bean id="fileIntegrityServiceRecFile" factory-bean="fileIntegrityServiceFactory" factory-method="createFileIntegrityService"	
	c:_0="REC" />
	
	<bean id="fileIntegrityServiceTdrFile" factory-bean="fileIntegrityServiceFactory" factory-method="createFileIntegrityService"	
	c:_0="TDR" />
	
	<bean id="fileIntegrityServiceWoFile" factory-bean="fileIntegrityServiceFactory" factory-method="createFileIntegrityService"	
	c:_0="WO" />
	
	<bean id="fileIntegrityServicePreRnbwFile" factory-bean="fileIntegrityServiceFactory" factory-method="createFileIntegrityService"	
	c:_0="PRE-RNBW" />
	
</beans>