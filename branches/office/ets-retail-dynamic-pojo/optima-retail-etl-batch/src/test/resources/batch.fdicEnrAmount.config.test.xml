<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:p="http://www.springframework.org/schema/p" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
                            http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.2.xsd
                            http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd">

	<context:annotation-config />

	<bean id="fdicHistoryProcessor"
		class="com.citi.retail.optima.etl.batch.job.processor.FDICEnrProcessor">
		<property name="preProcessorDataServiceFactory" ref="preProcessorDataServiceFactory" />
		<property name="batchInstanceId" value="100" />
	</bean>

	<bean id="preProcessorDataServiceFactory"
		class="com.citi.optima.retail.batch.preprocess.PreProcessorDataServiceFactoryImpl" />

	<bean id="preProcessorDataService" factory-bean="preProcessorDataServiceFactory"
		factory-method="getPreProcessorDataService" scope="step">
		<constructor-arg type="java.lang.Long" value="100" />
	</bean>

	<bean id="stepExecution" class="org.mockito.Mockito"
		factory-method="mock" primary="true">
		<constructor-arg value="org.springframework.batch.core.StepExecution" />
	</bean>
	
</beans>    