  <build>
	<plugins>
	 	<plugin>
			<groupId>org.apache.maven.plugins</groupId>
			<artifactId>maven-jar-plugin</artifactId>
			<configuration>
				<archive>
					<manifest>
						<mainClass>com.citi.risk.dashboard.DashboardApp</mainClass>
					</manifest>
				</archive>
			</configuration>
		</plugin>
	</plugins>
