<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">


<bean id="jndiTemplate" class="org.springframework.jndi.JndiTemplate">
	<property name="environment">
		<props>
			<prop key="java.naming.factory.initial">${ems.java.naming.factory.initial}</prop>
            <prop key="java.naming.provider.url">${ems.java.naming.provider.url}</prop>
            <prop key="java.naming.factory.url.pkgs">com.tibco.tibjms.naming</prop>
            <prop key="java.naming.security.principal">${ems.java.naming.security.principal}</prop>
			<prop key="com.tibco.tibjms.naming.security_protocol">${ems.java.security_protocol}</prop>
			<prop key="com.tibco.tibjms.naming.ssl_enable_verify_host">${ems.java.ssl_enable_verify_host}</prop>
			<prop key="com.tibco.tibjms.naming.ssl_identity">${ems.java.ssl_identity}</prop>
			<prop key="com.tibco.tibjms.naming.ssl_password">${ems.java.naming.ssl_password}</prop>
			<prop key="com.tibco.tibjms.ssl.password">${ems.ssl.password}</prop>
			<prop key="com.tibco.tibjms.naming.ssl_trusted_certs">${ems.java.naming.ssl_trusted_certs}</prop> 
			<prop key="com.tibco.tibjms.naming.ssl_trace">${ems.java.naming.ssl_trace}</prop>
			<prop key="com.tibco.tibjms.naming.ssl_debug_trace">${ems.java.naming.ssl_debug_trace}</prop>	
			<prop key="com.tibco.tibjms.naming.ssl_cipher_suites">${ems.java.naming.ssl_cipher_suites}</prop>		
		</props>
	</property>
</bean> 


