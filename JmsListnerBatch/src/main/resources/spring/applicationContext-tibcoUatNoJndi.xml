<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

<bean id="connectionFactory" class="com.tibco.tibjms.TibjmsConnectionFactory">
	<property name="serverUrl" value="ssl://gftsuat.nam.nsroot.net:7243" />
	<property name="userName" value="etsdashboard_167254" />
	<property name="userPassword" value="" />
	<property name="SSLVendor" value="j2se" />
  	<property name="SSLEnableVerifyHost" value="false" />
 	<property name="SSLEnableVerifyHostName" value="false" />
  	<property name="SSLTrace" value="true" />
  	<property name="SSLDebugTrace" value="false" />
  	<property name="SSLIdentity" value="src\main\resources\certs\etsdashboard_167254.p12" />
  	<property name="SSLPassword" value="CitiRisk@2014" />
  	<property name="SSLTrustedCertificate" value="src\main\resources\certs\entrust_root_dev.certs.pem" />	
</bean>


<bean id="cachingConnectionFactory" class="org.springframework.jms.connection.CachingConnectionFactory">
	<property name="targetConnectionFactory" ref="connectionFactory"></property>
	<property name="reconnectOnException" value="true"></property>
</bean>

<bean id="destination" class="com.tibco.tibjms.TibjmsQueue">
	<property name="address" value="ot.ets.na.ETSDashboard_167254.request" />
</bean>

<bean id="jmsTransManager" class="org.springframework.jms.connection.JmsTransactionManager">
	<property name="connectionFactory" ref="connectionFactory" />
</bean>

<bean id="jmsTemplate" class="org.springframework.jms.core.JmsTemplate">
	<property name="connectionFactory" ref="cachingConnectionFactory" />
	<property name="defaultDestination" ref="destination" />
</bean>

</beans>
